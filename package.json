{
  "name": "sheetjs",
  "description": "Easily create and manipulate CSSStyleDeclarations with javascript.",
  "version": "1.0.12",
  "keywords": [
    "CSSStyleDeclaration",
    "css",
    "javascript",
    "style",
    "sheetjs"
  ],
  "main": "dist/sheetjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/jshanson7/sheetjs"
  },
  "dependencies": {},
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "eslint": "^1.7.1",
    "eslint-loader": "^1.1.0",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "mocha": "^2.3.3",
    "mocha-loader": "^0.7.1",
    "object-assign": "^4.0.1",
    "object-keys": "^1.0.8",
    "parallelshell": "^2.0.0",
    "uglifyjs": "^2.4.10",
    "watch": "^0.16.0",
    "watchify": "^3.4.0",
    "webpack": "^1.12.2"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "node_modules/.bin/parallelshell 'npm run build-watch' 'npm run test-watch'",
    "test": "npm run lint && npm run test-browser && npm run test-node",
    "test-watch": "node_modules/.bin/parallelshell 'npm run lint-watch' 'npm run test-node-watch'",
    "test-browser": "open test/test.html",
    "test-node": "node_modules/.bin/mocha test/test",
    "test-node-watch": "node_modules/.bin/watch 'npm run test-node' test src",
    "build": "node_modules/.bin/parallelshell 'npm run browserify && npm run uglify' 'npm run browserify-modules' 'npm run browserify-test' 'npm run browserify-demo'",
    "build-watch": "node_modules/.bin/parallelshell 'npm run browserify-watch' 'npm run browserify-modules-watch' 'npm run browserify-test-watch' 'npm run browserify-demo-watch'",
    "browserify": "node_modules/.bin/browserify src/sheetjs.js > dist/sheetjs.js -t babelify -s SheetJS",
    "browserify-watch": "node_modules/.bin/watchify src/sheetjs.js -o dist/sheetjs.js -t babelify -s SheetJS -v",
    "browserify-modules": "node_modules/.bin/browserify src/StyleSheet.js > dist/StyleSheet.js -t babelify -s StyleSheet",
    "browserify-modules-watch": "node_modules/.bin/watchify src/StyleSheet.js -o dist/StyleSheet.js -t babelify -s StyleSheet -v",
    "browserify-test": "node_modules/.bin/browserify test/test.js > test/test.compiled.js -t babelify",
    "browserify-test-watch": "node_modules/.bin/watchify test/test.js -o test/test.compiled.js -t babelify -v",
    "browserify-demo": "node_modules/.bin/browserify demo/src/index.js > demo/dist/index.js -t babelify",
    "browserify-demo-watch": "node_modules/.bin/watchify demo/src/index.js -o demo/dist/index.js -t babelify -v",
    "uglify": "node_modules/.bin/uglifyjs dist/sheetjs.js -c -m -o dist/sheet.min.js",
    "lint": "node_modules/.bin/eslint src test --ignore-pattern ./**/*/*.{compiled,min}.js --quiet",
    "lint-watch": "node_modules/.bin/watch 'npm run lint' test src"
  },
  "author": "Jeff Hanson",
  "license": "MIT"
}
